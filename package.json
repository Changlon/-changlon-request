{
  "name": "request-any",
  "version": "2.0.2",
  "description": "ESM, CJS cross platform interface request lib",
  "author": "'Changlon' <'changlong.a2@gmail.com'> (https://github.com/Changlon/)",
  "license": "MIT",
  "repository": "https://github.com/Changlon/request-any",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "files": [
    "dist",
    "index.d.ts",
    "index.js",
    "index.test.js"
  ],
  "scripts": {
    "build": "tsup index.js --format cjs,esm  --clean",
    "watch": "npm run build -- --watch .",
    "prepublishOnly": "npm run build",
    "test": "mocha --require babel-register index.test.js -t 25000 " 
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "chai": "4.3.6",
    "mocha": "9.2.2",
    "tsup": "5.12.4",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "axios": "0.26.1",
    "js-md5": "0.7.3",
    "qs": "6.10.3"
  }
}
