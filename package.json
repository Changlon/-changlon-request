{
  "name": "@changlon/request-any",
  "version": "1.0.0",
  "description": "Send interface request across platforms",
  "author": "'Changlon' <'changlong.a2@gmail.com'> (https://github.com/Changlon/)",
  "license": "MIT",
  "repository": "https://github.com/Changlon/request-any",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup index.js --format cjs,esm --dts --clean",
    "watch": "npm run build -- --watch .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "tsup": "5.12.4",
    "typescript": "4.6.3"
  }
}
